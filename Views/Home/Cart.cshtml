@model IEnumerable<emarket.Models.Cart>

@{
    ViewBag.Title = "Index";
}

<h2>Cart</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Added_At)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Added_At)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Product_Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Product_Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Product_Id })
        </td>
    </tr>
}

</table>
